{
  "consumer": {
    "name": "dateConsumerKafka"
  },
  "provider": {
    "name": "dateProviderKafka"
  },
  "messages": [
    {
      "description": "valid date from kafka provider",
      "metaData": {
        "contentType": "application/json"
      },
      "contents": {
        "isLeapYear": true,
        "localDate": "2000-01-31"
      },
      "matchingRules": {
        "body": {
          "$.localDate": {
            "matchers": [
              {
                "match": "date",
                "date": "yyyy-MM-dd"
              }
            ],
            "combine": "AND"
          },
          "$.isLeapYear": {
            "matchers": [
              {
                "match": "type"
              }
            ],
            "combine": "AND"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.0.9"
    }
  }
}